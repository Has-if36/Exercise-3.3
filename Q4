#include <iostream>
#include <string>
using namespace std;

int main()
{
    double m;
    int v, ke;
    
    cout << "Input the object's mass (kg): ";
    cin >> m;
    cout << "Input the object's velocity (m/s): ";
    cin >> v;
    
    ke = m*v*v/2;
    
    cout << "The kinetic energy (Joules): " << ke;
}
